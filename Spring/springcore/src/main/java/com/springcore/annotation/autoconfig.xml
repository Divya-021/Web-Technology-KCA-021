<!-- Autowire Annotation -->
<context:annotation-config/>
<bean name="address" class="com.springcore.annotation.Address">
<property name="street" value="muradnagar"/>
<property name="city" value="Ghaziabad"/>
</bean>
<!-- used for qualifier Annotation when there are multiple beans-->
<bean name="address1" class="com.springcore.annotation.Address">
<property name="street" value="chopla"/>
<property name="city" value="Meerut"/>
</bean>

<bean name="emp1" class="com.springcore.annotation.Emp"/>
</beans>
<!-- Qualifier Annotation -->
  
